{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": ".",
        "paths": {
            "~/*": ["./*"],
            "@/*": ["./*"],
            "~~/*": ["./*"],
            "@@/*": ["./*"]
        }
    },
    "vueCompilerOptions": {
        "target": 2.7
    },
    "exclude": ["node_modules", ".nuxt", "dist"]
}
